{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Build Debug Gnu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Build/DebugGnu/RechnungsApp",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "BuildDebugGnu",
        },
        {
            "name": "Build Release Gnu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Build/ReleaseGnu/RechnungsApp",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "BuildReleaseGnu",
        },
        {
            "name": "Build Debug Clang",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/Build/DebugClang/RechnungsApp",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "BuildDebugClang"
        },
        {
            "name": "Build Release Clang",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/Build/ReleaseClang/RechnungsApp",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "BuildReleaseClang"
        }
    ]
}
